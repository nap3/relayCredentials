<?xml version="1.0" encoding="utf-8"?>
<SettingInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SettingVersion>2</SettingVersion>
  <ProxyServer>http://proxy.com:8080</ProxyServer>
  <ProxyUser>user</ProxyUser>
  <ProxyPassword>password</ProxyPassword>

  <!--Wild card(*) can not be used.    Examples  *.ff.com(NG)    aa.ff.com(OK) -->
  <BypassList>
    <string>aa.com</string>
    <string>bb.cc.com</string>
  </BypassList>

  <!--LogLevel is None, Error, or Full-->
  <LogLevel>None</LogLevel>
</SettingInfo>
