<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--dependentAssembly要素でdllを別の場所に移動させるためには厳密名が必要なよう。認証情報だけでも別の場所を指定できるようにする。-->
    <add key="relayCredentialsConfigPath" value="C:\Credentials\relayCredentialsSetting.xml"/>
  </appSettings>


  <system.net>
    <defaultProxy>
      <module type="relayCredentials.AuthProxyModule, relayCredentials"/>
    </defaultProxy>
  </system.net>
</configuration>
